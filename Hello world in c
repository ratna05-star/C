#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() 
{
    char s[100];
    
    // Print "Hello, World!" on first line
    printf("Hello, World!\n");
    
    // Read input string and print it on second line
    scanf("%[^\n]s", s);
    printf("%s\n", s);
    
    return 0;
}
